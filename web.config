<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <staticContent>
            <mimeMap fileExtension=".woff" mimeType="font/x-woff" />
            <mimeMap fileExtension=".woff2" mimeType="application/font-woff2" />
        </staticContent>
		<security>
		  <requestFiltering>  
		  <requestLimits maxQueryString="10240" />  
		  </requestFiltering>  
		</security> 
    </system.webServer>
	
</configuration>
